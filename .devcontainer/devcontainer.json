{
    "image": "docker.pkg.github.com/tomasaschan/vscode-devcontainers/haskell:latest",
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind",
        "source=/home/taschan/.ssh,target=/home/dev/.ssh-localhost,type=bind,readonly",
        "source=${localWorkspaceFolder}/.devcontainer/bashrc.d,target=/home/dev/.bashrc.d,type=bind,readonly",
    ],
    "extensions": [
        "haskell.haskell"
    ],
    "appPort": 3000
}